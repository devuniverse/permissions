{
    "name": "devuniverse/permissions",
    "description": "A package to manage permissions for laravel applications",
    "homepage": "https://github.com/devuniverse/permissions",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Permissions",
            "email": "ishimwe@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
          "Devuniverse\\Permissions\\": "/src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
      },
      "extra": {
          "laravel": {
              "providers": [
                  "Devuniverse\\Permissions\\PermissionsServiceProvider"
              ]
          }
      },
      "minimum-stability": "stable",
      "require-dev": {
          "laravel/laravel": "^6.0 || ^7.0 || ^8.0"
    }
}
